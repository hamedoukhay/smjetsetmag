<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Administrateur\AdminBundle\Entity\Cinema" table="cinema">
    <id name="id" type="integer" column="id_salle">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adresse" type="string" column="adresse" length="50" nullable="true"/>
    <field name="emplacement" type="string" column="emplacement" length="1000" nullable="true"/>
    <field name="nomSalle" type="string" column="nom_salle" length="70" nullable="true"/>
    <field name="coordx" type="float" column="coordx" precision="10" scale="0" nullable="true"/>
    <field name="coordy" type="float" column="coordy" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
