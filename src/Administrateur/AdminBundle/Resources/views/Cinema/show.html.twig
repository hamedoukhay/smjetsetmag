{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Cinema</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Adresse</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
                <th>Emplacement</th>
                <td>{{ entity.emplacement }}</td>
            </tr>
            <tr>
                <th>Nomsalle</th>
                <td>{{ entity.nomSalle }}</td>
            </tr>
            <tr>
                <th>Coordx</th>
                <td>{{ entity.coordx }}</td>
            </tr>
            <tr>
                <th>Coordy</th>
                <td>{{ entity.coordy }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cinema') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cinema_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
